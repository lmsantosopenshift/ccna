<component id="ID_s3_title">
  <text><![CDATA[<b>Enabling OSPF MD5 Authentication</b>]]></text> 
  </component>
  
<!--syntax  checker --> 
	<component id="ID_sc_reset">
		<text><![CDATA[Reset]]></text>
	</component>
	<component id="ID_sc_showme">
